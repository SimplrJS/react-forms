        "webpack-cli": "3.3.9",
        "webpack-dev-server": "3.8.2"
    },
    "jest": {
        "verbose": true,
        "preset": "ts-jest",
        "reporters": [
            "default",
            "jest-junit"
        ],
        "collectCoverage": true,
        "testRegex": "/__tests__/.*\\.(test|spec).(ts|tsx)$",
        "collectCoverageFrom": [
            "src/**/*.{ts,tsx}",
            "!src/**/__tests__/*",
            "!src/index.ts"
        ],
        "coverageReporters": [
            "cobertura"
        ]
    }
