        "@types/terser-webpack-plugin": "^1.2.1",
        "@types/webpack": "^4.39.7",
        "@types/webpack-dev-server": "^3.4.0",
        "cross-env": "6.0.3",
        "eslint": "6.6.0",
        "node-sass": "4.13.0",
        "typescript": "3.7.2",
        "webpack": "4.41.2",
        "webpack-cli": "3.3.9",
        "webpack-dev-server": "3.9.0"
    },
    "jest": {
        "verbose": true,
        "preset": "ts-jest",
        "reporters": [
            "default",
            "jest-junit"
        ],
        "collectCoverage": true,
        "testRegex": "/__tests__/.*\\.(test|spec).(ts|tsx)$",
        "collectCoverageFrom": [
            "src/**/*.{ts,tsx}",
            "!src/**/__tests__/*",
            "!src/index.ts"
        ],
        "coverageReporters": [
            "cobertura"
        ]
    }
