    "scripts": {
        "start": "npm run build:tools && cross-env NODE_ENV=development webpack-dev-server --host 127.0.0.1",
        "build": "npm run build:tools && cross-env NODE_ENV=development webpack",
        "build:tools": "tsc -p ./tools"
    },
    "files": [
        "dist",
        "**/*.md"
    ],
    "dependencies": {
        "@reactway/forms": "^1.0.0",
        "core-js": "^3.4.1",
        "react": "16.11.0",
        "react-dom": "16.11.0"
    },
    "devDependencies": {
        "@reactway/eslint-config": "1.0.0-alpha.5",
        "@reactway/webpack-builder": "2.0.1",
        "@reactway/webpack-builder-plugin-clean": "2.0.1",
        "@reactway/webpack-builder-plugin-html": "2.0.1",
        "@reactway/webpack-builder-plugin-images": "2.0.1",
        "@reactway/webpack-builder-plugin-styles": "2.0.1",
        "@reactway/webpack-builder-plugin-typescript": "2.0.1",
        "@reactway/webpack-builder-plugin-web-dev": "2.0.1",
        "@reactway/webpack-builder-plugin-write-file": "2.0.1",
        "@types/html-webpack-plugin": "^3.2.1",
